#include<iostream>
#include<string>
using namespace std;
int main()
{
string c[15];
cout<<"Vuvedete imenata na suychenicite si po nomer v klasa."<<endl;
for(int i=0;i<15;i++)
{
getline(cin,c[i]);
}
cout<<"Imenata na suychenicite vi sa: "<<endl;
for (int i=0; i<15; i++)
{
cout <<c[i]<<endl;
}
system("pause>nul");
return 0;
}
