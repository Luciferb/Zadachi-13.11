#include<iostream>
#include<string>
using namespace std;
int main()
{
int n,max,min;
cout<<"Vuvedete kolko priqteli imate."<<endl;
cin>>n;
int p[100];
cout<<"Vuvedete, na kolko godini sa priqtelite vi."<<endl;
for(int i=0;i<n;i++)
{
cin>>p[i];
}
max=p[0];
min=p[0];
for(int i=0;i<n;i++)
{
if(p[i]>max)max=p[i];
if(p[i]<min)min=p[i];
}
cout<<"Nai-stariqt e na "<<max<<" godini, a nai-malkiqt e na "<<min<<" godini."<<endl;
system("pause>nul");
return 0;
}
